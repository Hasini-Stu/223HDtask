    </suppress>
    
    <!-- Suppress false positives for testing libraries -->
    <suppress>
        <notes><![CDATA[
        Testing libraries false positives - these are only used in test environment
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito.*$</packageUrl>
        <vulnerabilityName>CVE-2021-29425</vulnerabilityName>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Testing libraries false positives - these are only used in test environment
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.robolectric.*$</packageUrl>
        <vulnerabilityName>CVE-2021-29425</vulnerabilityName>
    </suppress>
    
</suppressions>
